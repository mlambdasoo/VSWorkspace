{
  "id": "com.sudong.addon.ex1.setter",
  "version": "1.0.0",
  "name": "Sudong Widget add-on setter test ",
  "description": "A widget customization add-on basic setter test",
  "icon": "undefined",
  "vendor": "SAP",
  "eula": "EULA",
  "license": "1.0",
  "extensions": [
    {
      "extensionPoint": "sap.addOn.viz.plotarea.general",
      "webcomponents": [
        {
          "kind": "main",
          "tag": "exercise-one",
          "url": "https://mlambdasoo.github.io/VSWorkspace/4_WidgetAddOn/Ex1/ex1.js",
          "integrity": "",
          "ignoreIntegrity": true
        },
        {
          "kind": "builder",
          "tag": "exercise-one-builder",
          "url": "https://mlambdasoo.github.io/VSWorkspace/4_WidgetAddOn/Ex1/ex1-builder.js",
          "integrity": "",
          "ignoreIntegrity": true
        }
      ],
      "properties": {
        "sapHideOriginalDataPointMarks": {
          "type": "boolean",
          "default": false
        },
        "sapHideOriginalDataPointLabels": {
          "type": "boolean",
          "default": false
        },
        "sapHideOriginalXAxisLabels": {
          "type": "boolean",
          "default": false
        },
        "sapHideOriginalYAxisLabels": {
          "type": "boolean",
          "default": false
        },
        "sapHideOriginalXAxisStackLabels": {
          "type": "boolean",
          "default": false
        },
        "sapHideOriginalYAxisStackLabels": {
          "type": "boolean",
          "default": false
        },
        "lineColor": {
          "type": "string",
          "default": "default"
        },
        "measures": {
          "type": "measureSetting",
          "default": { "itemColor": "#93C939", "lineOpacity": 0.2 }
        }
      },
      "types": {
        "measureSetting": [
          {
            "description": "type for styling settings",
            "properties": [
              {
                "name": "measureName",
                "type": "string",
                "description": "Measure Name"
              },
              {
                "name": "dotted",
                "type": "boolean",
                "description": "dotted or default"
              },
              {
                "name": "dotted color",
                "type": "string",
                "description": "hex number"
              },
              {
                "name": "dotted color",
                "type": "string",
                "description": "hex number"
              }
            ]
          }
        ]
      }
    }
  ]
}
